@page "/admin/questionnaires/{questionnaireId}/look-and-feel"
@model Admin.Pages.Customisations.QuestionnaireLookAndFeel

@{
    ViewData["Title"] = "Branching map";
}

@section BeforeContent
{
    <a class="govuk-back-link govuk-!-display-none-print" href="/admin/questionnaires/@Model.Questionnaire.Id/track">Back to questionnaire '@Model.Questionnaire.Title'</a>
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-l">
            <span class="govuk-caption-l">@Model.Questionnaire.Title</span>
            Branching map
        </h1>

        <div class="govuk-hint">This visualises question-to-answer branching. Answersâ€™ scores only indicate prioritisation and do not affect routing.</div>

        <div class="govuk-!-margin-top-4">
            
        </div>
    </div>
</div>